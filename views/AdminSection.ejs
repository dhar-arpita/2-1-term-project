    <script>
        // Function to display the message if it exists
        function showMessage(message) {
          if (message) {
            $('#messageText').text(message); // Set the message text
            $('#messageModal').modal('show'); // Show the modal
          }
        }
  
        // Example usage:
        // Call showMessage function with your message
        var message = '<%= typeof message !== "undefined" ? message : "" %>'; // Your message here
        showMessage(message);
      </script>
